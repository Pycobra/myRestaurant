{
    "builds": [
        {
            "src": "myRestaurant/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.8"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "myRestaurant/wsgi.py"
        }
    ]
}
